{
  "name": "boda",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "yarn redirects:enable && remix build",
    "dev": "yarn redirects:disable && remix dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "redirects:disable": "shx rm -f public/_redirects",
    "redirects:enable": "shx cp .redirects public/_redirects",
    "start": "netlify serve",
    "test": "NODE_ENV=test vitest",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@netlify/functions": "^2.4.1",
    "@netlify/remix-adapter": "^2.0.0",
    "@remix-run/node": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "@remix-run/serve": "^2.4.1",
    "isbot": "^3.7.1-deprecated",
    "match-sorter": "^6.3.1",
    "react": "^18.3.0-canary-c5b937576-20231219",
    "react-dom": "^18.3.0-canary-c5b937576-20231219",
    "shx": "^0.3.4",
    "sort-by": "^1.2.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.4.1",
    "@remix-run/eslint-config": "^2.4.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-remix-react-routes": "^1.0.5",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "tailwindcss": "^3.4.0",
    "typescript": "5.1.6",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "app/**/*.{ts,tsx,json}": [
      "eslint --max-warnings 0 --fix"
    ],
    "app/**/*.{ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
