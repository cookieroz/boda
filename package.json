{
  "name": "boda",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "yarn redirects:enable && remix build",
    "dev": "yarn redirects:disable && remix dev",
    "start": "netlify serve",
    "typecheck": "tsc -b",
    "redirects:enable": "shx cp .redirects public/_redirects",
    "redirects:disable": "shx rm -f public/_redirects"
  },
  "dependencies": {
    "@netlify/functions": "^2.0.0",
    "@netlify/remix-adapter": "^2.0.0",
    "@remix-run/node": "^2",
    "@remix-run/react": "^2",
    "@remix-run/serve": "^2",
    "isbot": "^3.6.13",
    "match-sorter": "^6.3.1",
    "react": "^18.3.0-canary-08a39539f-20231031",
    "react-dom": "^18.3.0-canary-08a39539f-20231031",
    "shx": "^0.3.4",
    "sort-by": "^1.2.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2",
    "@remix-run/eslint-config": "^2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
