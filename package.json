{
  "name": "boda",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "yarn redirects:enable && remix build",
    "dev": "yarn redirects:disable && remix dev",
    "start": "netlify serve",
    "typecheck": "tsc -b",
    "redirects:enable": "shx cp .redirects public/_redirects",
    "redirects:disable": "shx rm -f public/_redirects"
  },
  "dependencies": {
    "@netlify/functions": "^2.4.1",
    "@netlify/remix-adapter": "^2.0.0",
    "@remix-run/node": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "@remix-run/serve": "^2.4.1",
    "isbot": "^3.7.1-deprecated",
    "match-sorter": "^6.3.1",
    "react": "^18.3.0-canary-c5b937576-20231219",
    "react-dom": "^18.3.0-canary-c5b937576-20231219",
    "shx": "^0.3.4",
    "sort-by": "^1.2.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.4.1",
    "@remix-run/eslint-config": "^2.4.1",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
